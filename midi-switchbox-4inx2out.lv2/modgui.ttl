@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/midi-switchbox-4inx2out>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-midi-switchbox2.html> ;
        modgui:stylesheet <modgui/stylesheet-midi-switchbox2.css> ;
        modgui:screenshot <modgui/screenshot-midi-switchbox2.png> ;
        modgui:thumbnail <modgui/thumbnail-midi-switchbox2.png> ;
        modgui:brand "MOD" ;
        modgui:label "MIDI SwitchBox2" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "black" ;
        modgui:knob "gold" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "target" ;
            lv2:name "Target" ;
        ] ;
    ] .
